  },
  "build": {
    "free": {
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "free"
      }
    },
    "pro": {
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "pro"
      }
    },
    "freeStore": {
      "distribution": "store",
      "env": {
        "APP_VARIANT": "free"
      }
    },
    "proStore": {
      "distribution": "store",
      "env": {
        "APP_VARIANT": "pro"
      }
    }
  }
}